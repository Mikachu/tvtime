"                                 us-cable\n"
"                                 us-cable100\n"
"                                 us-broadcast\n"
"                                 china-broadcast\n"
"                                 japan-cable\n"
"                                 japan-broadcast\n"
"                                 europe\n"
"                                 australia\n"
"                                 australia-optus\n"
