"  -i, --input=INPUTNUM       video4linux input number (defaults to 0).\n"
msgstr ""
"  -i, --input=SIGNALNUMMER   video4linux signalnummer (0 om ej "
"specifierad).\n"

#: src/tvtimeconf.c:668 src/tvtimeconf.c:721
msgid ""
"  -I, --inputwidth=SAMPLING  video4linux input scanline sampling\n"
"                             (defaults to 720).\n"
msgstr ""
"  -I, --inputwidth=SAMPLING  Vertikal bildupplösning (720 om ej "
"specifierad).\n"

