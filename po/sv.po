#: src/tvtimeconf.c:626
#, c-format
msgid ""
"usage: %s [OPTION]...\n"
"\n"
"  -a, --widescreen           16:9 mode.\n"
"  -b, --vbidevice=DEVICE     VBI device (defaults to /dev/vbi0).\n"
"  -c, --channel=CHANNEL      Tune to the specified channel on startup.\n"
"  -d, --device=DEVICE        video4linux device (defaults to /dev/video0).\n"
"  -D, --driver=NAME          Output driver to use: Xv, DirectFB, mga,\n"
"                             xmga, matroxtv or SDL (defaults to Xv).\n"
"  -f, --frequencies=NAME     The frequency table to use for the tuner.\n"
"                             (defaults to us-cable).\n"
"\n"
"                             Valid values are:\n"
"                                 us-cable\n"
"                                 us-cable100\n"
"                                 us-broadcast\n"
"                                 china-broadcast\n"
"                                 japan-cable\n"
"                                 japan-broadcast\n"
"                                 europe\n"
"                                 australia\n"
"                                 australia-optus\n"
"                                 newzealand\n"
"                                 france\n"
"                                 russia\n"
"                                 custom (first run tvtime-scanner)\n"
"\n"
"  -F, --configfile=FILE      Additional config file to load settings from.\n"
"  -h, --help                 Show this help message.\n"
"  -H, --height=HEIGHT        Output window height (defaults to 576).\n"
"  -i, --input=INPUTNUM       video4linux input number (defaults to 0).\n"
"  -I, --inputwidth=SAMPLING  video4linux input scanline sampling\n"
"                             (defaults to 720).\n"
"  -k, --slave                Disables input handling in tvtime (slave "
"mode).\n"
"  -m, --fullscreen           Start tvtime in fullscreen mode.\n"
"  -M, --window               Start tvtime in window mode.\n"
"  -n, --norm=NORM            The norm to use for the input.  tvtime "
"supports:\n"
"                             NTSC, NTSC-JP, SECAM, PAL, PAL-Nc, PAL-M,\n"
"                             PAL-N or PAL-60 (defaults to NTSC).\n"
"  -p, --fspos=POS            Set the fullscreen position: top, bottom or\n"
"                             centre (default).\n"
"  -r, --rvr=FILE             RVR recorded file to play (for debugging).\n"
"  -s, --showdrops            Print stats on frame drops (for debugging).\n"
"  -S, --saveoptions          Save command line options to the config file.\n"
"  -t, --xmltv=FILE           Read XMLTV listings from the given file.\n"
"  -v, --verbose              Print debugging messages to stderr.\n"
"  -x, --mixer=DEVICE[:CH]    The mixer device and channel to control.\n"
"                             (defaults to /dev/mixer:line)\n"
"\n"
"                             Valid channels are:\n"
"                                 vol, bass, treble, synth, pcm, speaker, "
"line,\n"
"                                 mic, cd, mix, pcm2, rec, igain, ogain, "
"line1,\n"
"                                 line2, line3, dig1, dig2, dig3, phin, "
"phout,\n"
"                                 video, radio, monitor\n"
msgstr ""
"användning: %s [FLAGGA]...\n"
"\n"
"  -a, --widescreen           16:9-läge.\n"
"  -b, --vbidevice=ENHET      VBI-enhet (förval: /dev/vbi0).\n"
"  -c, --channel=KANAL        Aktivera specifierad kanal vid uppstart.\n"
"  -d, --device=ENHET         video4linux-enhet (förval: /dev/video0).\n"
"  -D, --driver=NAMN          Specifiera drivrutin för utmatning:\n"
"                             Xv, DirectFB, mga, xmga, matroxtv eller SDL\n"
"                             (förvalt till Xv).\n"
"  -f, --frequencies=NAMN     Specifiera vilken frekvenstabell som ska\n"
"                             användas för den här tunern.\n"
"                             (förval: us-cable).\n"
"\n"
"                             Giltiga värden är:\n"
"                                 us-cable\n"
"                                 us-cable100\n"
"                                 us-broadcast\n"
"                                 china-broadcast\n"
"                                 japan-cable\n"
"                                 japan-broadcast\n"
"                                 europe\n"
"                                 australia\n"
"                                 australia-optus\n"
"                                 newzealand\n"
"                                 france\n"
"                                 russia\n"
"                                 custom (kör tvtime-scanner först)\n"
"\n"
"  -F, --configfile=FIL       En ytterligare fil att ladda inställningar "
"från.\n"
"  -h, --help                 Visa det här hjälpmeddelandet.\n"
"  -H, --height=HÖJD          Visningsfönstrets höjd (förval: 576).\n"
"  -i, --input=INDATANR       video4linux indatanummer (förval: 0).\n"
"  -I, --inputwidth=SAMPLING  video4linux scanline-sampling (förval: 720)\n"
"  -k, --slave                Stänger av indatahantering (slav-läge).\n"
"  -m, --fullsreen            Starta tvtime i fullskärmsläge.\n"
"  -M, --window               Starta tvtime i fönsterläge.\n"
"  -n, --norm=NORM            TV-norm att använda för indata. tvtime "
"stödjer:\n"
"                             NTSC, NTSC-JP, SECAM, PAL, PAL-Nc, PAL-M,\n"
"                             PAL-N och PAL-60 (förval: NTSC).\n"
"  -p, --fspos=POS            Ställ in fullskärmsposition:\n"
"                             topp, botten, eller mitten (förval)\n"
"  -r, --rvr=FILE             Spela upp RVR-fil (för avlusning).\n"
"  -s, --showdrops            Visa status för tappade bildrutor (för "
"avlusning)\n"
"  -S, --saveoptions          Spara kommandoradsflaggor i "
"konfigurationsfilen.\n"
"  -t, --xmltv=FIL            Läs XMLTV-tablåer från en fil.\n"
"  -v, --verbose              Skriv ut avlusningsmeddelanden till stderr.\n"
"  -x, --mixer=ENHET[:KANAL]  Mixer-enhet och kanal att styra.\n"
"                             (förval: /dev/mixer:line)\n"
"\n"
"                             Giltiga kanaler är:\n"
"                                 vol, bass, treble, synth, pcm, speaker, "
"line,\n"
"                                 mic, cd, mix, pcm2, rec, igain, ogain, "
"line1,\n"
"                                 line2, line3, dig1, dig2, dig3, phin, "
"phout,\n"
"                                 video, radio, monitor\n"

